{
    "name": "davidpella/beem-sms",
    "description": "Provides Beem notification channel for Laravel",
    "keywords": [
        "laravel",
        "beem",
        "notification",
        "sms",
        "davidpella",
        "beem-sms"
    ],
    "homepage": "https://github.com/davidpella/beem-sms",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "David Pella",
            "email": "pella.code@gmail.com",
            "homepage": "https://github.com/davidpella",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/support": "^8.50"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "orchestra/testbench": "^6.15"
    },
    "autoload": {
        "psr-4": {
            "DavidPella\\BeemSms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DavidPella\\BeemSms\\Tests\\": "src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:unit": "phpunit --verbose --testsuite Unit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DavidPella\\BeemSms\\BeemSmsServiceProvider"
            ],
            "aliases": {
                "BeemSms": "DavidPella\\BeemSms\\Facades\\BeemSms"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
